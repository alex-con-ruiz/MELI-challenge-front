{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "react-app",
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "trailingComma": "es5",
        "endOfLine":"auto",
        "semi": false,
        "jsxSingleQuote": true,
        "singleQuote": true
      }
    ]
  }
}
